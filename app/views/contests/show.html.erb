<div class= "row">
  <div class="col-md-9">
  <%= link_to "Contest scoreboard", contest_scoreboard_path(@contest), class: "btn btn-info"%>
  <%= link_to "Contest submission table", contest_submissions_path(@contest), class: "btn btn-info"%>
    <div class="table-responsive">
      <table class="table bg-light table-striped">
        <thead class="bg-light">
        <tr>
          <th>ID</th>
          <th>Problem name</th>
          <th>Time Limit</th>
          <th>Memory Limit</th>
          </tr>
          </thead>
          <% @contest.problems.each do |probs| %>
            <tr>
              <td><%= link_to probs.id, contest_problem_path(@contest, probs) %></td>
              <td><%= link_to probs.name.titleize, contest_problem_path(@contest, probs) %></td>
              <td><%= probs.timelimit%></td>
              <td><%= probs.memlimit%></td>
            </tr>
          <% end %>
      </table>
    </div>
    </div>

  <div class="col-md-3">
         <%= render 'shared/new_exercise'%>
        </div>
  </div>